<div class="section">
  <div id="submitform">
    <% form_for @list do |f| %>
      <%= f.error_messages %>
      <%= f.label :name, "List Name" %>
      <%= f.text_field :name %>
      <%= f.label :admin_email, "Your Email" %>
      <%= f.text_field :admin_email %>
      <%= f.submit "Create" %>
    <% end %>
  </div>
  <div id="contact-info">
    <p>Quickly create and easily manage email distributions lists</p>
    <p>To start name you distribution list below along with your email address</p>
  </div>
  <div class="fix" ></div>
</div>
